@model  APIClientTool.ViewModels.Form941SCHR.Form941SCHRStatusResponse
<div class="modal-header edit-modal-header">
    <button type="button" class="close" data-dismiss="modal"></button>
    <h4 id="form_heading" class="modal-title">
        Efile Status
    </h4>
</div>
<div class="modal-body edit-modal-body white_bg">
    @if (Model != null && Model.StatusCode > 0)
    {
        <table class="responsive">
            <tr>
                <td class="labelName"><label class="control-label">SubmissionId:</label></td>
                <td class="fieldText">@Model.SubmissionId</td>
            </tr>
            <tr><td class="spacer10"></td></tr>
            <tr>
                <td class="labelName"><label class="control-label">Status Code:</label></td>
                <td class="fieldText">@Model.StatusCode</td>
            </tr>
            <tr><td class="spacer10"></td></tr>
            <tr>
                <td class="labelName"><label class="control-label">Status Message:</label></td>
                <td class="fieldText">@Model.StatusMessage</td>
            </tr>
            <tr><td class="spacer10"></td></tr>
            <tr>
                <td class="labelName"><label class="control-label">Status Name:</label></td>
                <td class="fieldText">@Model.StatusName</td>
            </tr>
        </table>
    }
    @if (Model != null && Model.Form941SchRRecords != null && Model.Form941SchRRecords.SuccessRecords != null)
    {
        <div class="spacer15"> </div>
            <h3 style="font-size:medium;font-weight:600">Success Records</h3>
            <div>
                <table class="table table-bordered">
                    <thead class="thead-inverse">
                        <tr>
                            <th>Sequence</th>
                            <th>Record Id</th>
                            <th>Record Status</th>
                            <th>Date Created On</th>
                            <th>Date Updated On</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.Form941SchRRecords.SuccessRecords)
                        {
                            <tr>
                                <td>@item.Sequence</td>
                                <td>@item.RecordId</td>
                                <td>@item.RecordStatus</td>
                                <td>@item.CreatedTs</td>
                                <td>@item.UpdatedTs</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
    }

    @if (Model != null && Model.Errors != null)
    {
        <h3 style="font-size:medium;font-weight:600">Error Records</h3>
            <div class="spacer15"> </div>
            <table class="table table-bordered">
                <thead class="thead-inverse">
                    <tr>
                        <th>Code</th>
                        <th>Name</th>
                        <th>Message</th>
                        <th>Type</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var itemerror in Model.Errors)
                {
                        <tr>
                            <td>@itemerror.Code</td>
                            <td>@itemerror.Name</td>
                            <td>@itemerror.Message</td>
                            <td>@itemerror.Type</td>
                        </tr>
                    }
                </tbody>
            </table>
    }
</div>

<div class="modal-footer edit-modal-footer mTopNo">
    <div class="taC">
        <button type="button" class="btn btn_md btn_second" data-dismiss="modal">
            Okay
        </button>
    </div>
</div>

<script type="text/javascript">
    $(function () {


    })
</script>
